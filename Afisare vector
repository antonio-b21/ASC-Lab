.data
	v: .word 15, 239, 34, 998, 342, 998, 324, 998, 24, 13
	n: .word 10
	sp: .asciiz " "
.text
main:
	lw $t0, n
	li $t1, 0
	li $t2, 0

loop:
	bge $t1, $t0, exit
	lw $a0, v($t2)
	li $v0, 1
	syscall
	la $a0, sp
	li $v0, 4
	syscall

	addi $t1, $t1, 1
	addi $t2, $t2, 4
	j loop
exit:
	li $v0, 10
	syscall
